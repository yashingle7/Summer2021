import cv2
import numpy as np


photo = np.zeros((500,500,3))
#face
photo =cv2.circle(photo, center=(250,130), radius=100, color=(0,255,255), thickness= -1)
#eyes
photo =cv2.circle(photo, center=(220,100), radius=20, color=(255,255,255), thickness= -1)
photo =cv2.circle(photo, center=(220,100), radius=8, color=(0,0,0), thickness= -1)
photo =cv2.circle(photo, center=(280,100), radius=20, color=(255,255,255), thickness= -1)
photo =cv2.circle(photo, center=(280,100), radius=8, color=(0,0,0), thickness= -1)
photo =cv2.circle(photo, center=(250,170), radius=22, color=(0,0,255), thickness= -1)


cv2.imshow("face", photo)
cv2.waitKey()
cv2.destroyAllWindows()
